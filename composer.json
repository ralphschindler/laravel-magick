{
    "name": "ralphschindler/laravel-magick",
    "description": "A media library for Laravel with a big imagick flex",
    "keywords": ["laravel", "media", "eloquent", "imagick"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ralph Schindler",
            "email": "ralph@ralphschindler.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMagick\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelMagick\\Test\\": "tests/"
        }
    },
    "suggest": {
        "ext-imagick": "Imagick image modifications",
        "laravel/framework": "This package is intended to be used with laravel/framework version >= 5.7"
    },
    "conflict": {
        "laravel/framework": "<5.7.0",
        "laravel/nova": "<2.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMagick\\MagickProvider"
            ]
        }
    }
}
